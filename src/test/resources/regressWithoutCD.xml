    <test name="TestMIS_Chrome" verbose="1">
        <parameter name="browser" value="chrome"/>
        <groups>
            <run>
                <include name="mis"/>
            </run>
        </groups>
        <classes>
            <class name="emias.mis.base.PerehodyTest"/>
        </classes>
