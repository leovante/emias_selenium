    <!--Диспансеризация-->
    <test name="TestDisp_Chrome" verbose="1">
        <parameter name="browser" value="chrome"/>
        <groups>
            <run>
                <include name="disp"/>
            </run>
        </groups>
        <packages>
            <package name="emias.disp.base"/>
        </packages>
    </test>
    <!--Медицинские записи-->
    <test name="TestMR_Chrome" verbose="1">
        <parameter name="browser" value="chrome"/>
        <groups>
            <run>
                <include name="MR"/>
