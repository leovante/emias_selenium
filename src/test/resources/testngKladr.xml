<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Name suite - KL" thread-count="2" parallel="classes">
    <parameter name="gridRun" value="true"/>

    <listeners>
        <listener class-name="com.utils.testngRetryCount.AnnotationTransformer"/>
        <listener class-name="com.utils.FirstListner"/>
    </listeners>

    <test name="ChromeTest" verbose="1" parallel="true">
        <parameter name="browser" value="chrome"/>
        <groups>
            <run>
                <include name="kl"/>
            </run>
        </groups>
        <packages>
            <package name="emias.kladr"/>
        </packages>
    </test>
</suite>