<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Name suite - Disp" thread-count="2" parallel="classes">
    <parameter name="gridRun" value="true"/>
    <parameter name="testng" value="true"/>

    <listeners>
        <listener class-name="com.utils.testngRetryCount.AnnotationTransformer"/>
        <listener class-name="com.utils.CustomListner1"/>
    </listeners>

    <test verbose="1" name="ChromeTest">
        <parameter name="browser" value="chrome"/>
        <groups>
            <run>
                <include name="disp"/>
            </run>
        </groups>
        <packages>
            <package name="emias.disp.base"/>
        </packages>
    </test>
</suite>