<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Name suite - Disp">
    <parameter name="gridRun" value="true"/>

    <listeners>
        <listener class-name="utils.testngRetryCount.AnnotationTransformer"/>
    </listeners>

    <test name="BeforeTest" verbose="1">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="emias.disp.before.BeforeSuiteDisp"/>
        </classes>
    </test>
    <test verbose="1" name="ChromeTest">
        <parameter name="browser" value="chrome"/>
        <groups>
            <run>
                <include name="disp"/>
            </run>
        </groups>
        <classes>
            <class name="emias.disp.before.BeforeSuiteDisp"/>
        </classes>
        <!--<packages>-->
        <!--<package name="emias.disp.regress"/>-->
        <!--</packages>-->
    </test>
    <test name="AfterTest" verbose="1">
        <parameter name="browser" value="chrome"/>
        <packages>
            <package name="emias.disp.after"/>
        </packages>
    </test>
</suite>