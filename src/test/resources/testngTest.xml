<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Name suite - Test" thread-count="5" parallel="classes">
    <parameter name="gridRun" value="true"/>

    <listeners>
        <listener class-name="com.utils.testngRetryCount.AnnotationTransformer"/>
    </listeners>

    <test name="ChromeTest" verbose="1" parallel="true">
        <parameter name="browser" value="chrome"/>
        <groups>
            <run>
                <include name="test"/>
            </run>
        </groups>
        <classes>
            <class name="emias.calldoctor.negative.ClickBoosterTest"/>
        </classes>
        <!--<packages>-->
        <!--<package name="emias.calldoctor22.base"/>-->
        <!--<package name="emias.calldoctor22.function"/>-->
        <!--</packages>-->
    </test>
</suite>