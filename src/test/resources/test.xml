<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Name suite - CD" thread-count="2" parallel="classes">
    <parameter name="gridRun" value="true"/>

    <listeners>
        <!--<listener class-name="com.commonsns.screenshotsListner.CustomTestListener"/>-->
        <listener class-name="com.commons.retryCountListner.AnnotationTransformer"/>
        <!--<listener class-name="com.epam.reportportal.testng.ReportPortalTestNGListener"/>-->
    </listeners>

    <test name="TestMIS_Chrome" verbose="1">
        <parameter name="browser" value="chrome"/>
        <groups>
            <run>
                <include name="mis"/>
            </run>
        </groups>
        <classes>
            <class name="emias.mis.base.PerehodyTest"/>
        </classes>
    </test>
</suite>