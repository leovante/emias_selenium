select s.Name, sm.serviceMedicalid
from hlt_disp_exam exam
inner join hlt_disp_CARD card on exam.rf_CardGuid = card.Guid
inner join hlt_disp_Service s on exam.rf_ServiceGuid = s.Guid
inner join hlt_disp_ServicePM spm on spm.Guid = exam.rf_ServicePMGuid
inner join hlt_disp_ServiceSM sms on spm.rf_PatientModelGuid = sms.rf_PatientModelGuid and sms.rf_ServiceGuid = exam.rf_ServiceGuid
inner join oms_serviceMedical sm on sms.rf_OmsServiceMedicalGuid = sm.guidsm
where spm.IsRequiredSM = 1 and card.disp_CardID = 2201




declare @ress varchar(5000), @date date
    set @date = '2019-01-06'
    exec hlt_getDocPRVDFilter 0, 0, 0, 11, 2, 13163, @date, @ress output
    select @ress


    select rf_kl_DDServiceID, rf_kl_ReasonTypeID, rf_PRVSID,*
    from oms_Tariff
    where rf_ServiceMedicalID = 13163 and
        rf_kl_DDServiceID = 2 and
        rf_kl_ReasonTypeID = 11 and
        @date between date_b and date_e




update oms_Tariff
set rf_kl_ReasonTypeID = 11, rf_kl_DDServiceID = 2 , rf_PRVSID = 1282, rf_ServiceMedicalID=26418
where tariffid = (select top 1 TariffID from oms_Tariff where TariffID > 0 and  rf_kl_ReasonTypeID = 0 and rf_kl_DDServiceID = 0 and rf_PRVSID = 0 and date_e > GetDate())


select * from oms_prvs prvs
inner join hlt_DocPRVD prvd on prvs.prvsid = prvd.rf_prvsid
and prvd.D_End > GETDATE()